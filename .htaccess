RewriteEngine On
RewriteBase /laniazip/

# Skip real files and directories
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Automatically route clean URLs to module.php
# Example: /contact â†’ /module.php?modname=contact
RewriteRule ^([^/]+)/?$ module.php?modname=$1 [L,QSA]
RewriteRule ^news/([^/]+)/([0-9]+)/?$ module.php?modname=news&mf=$1&cid=$2 [L,QSA]
RewriteRule ^content/([0-9]+)/?$ module.php?modname=content&cid=$1 [L,QSA]
# SEO-friendly search URLs
RewriteRule ^search/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/(.+)/?$ module.php?modname=search&item=$1&method=$2&keyword=$3 [L,QSA]
RewriteRule ^search/([a-zA-Z0-9_-]+)/(.+)/?$ module.php?modname=search&item=$1&keyword=$2 [L,QSA]